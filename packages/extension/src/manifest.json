{
  "manifest_version": 2,
  "name": "Deep Reading",
  "version": "0.0.1",
  "description": "",
  "icons": {
    "48": "./logo.png"
  },
  "background": {
    "scripts": ["background.chunk.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.chunk.js"]
    }
  ],
  "browser_action": {
    "default_title": "Deep Reading",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
