{
    "name": "@wrp/core",
    "version": "0.0.1",
    "private": true,
    "author": "Baotlake baotlake@gmail.com",
    "license": "SEE LISENSE IN LISENSE.md",
    "scripts": {
        "start:esbuild": "tsc --emitDeclarationOnly & NODE_ENV=development node build.js",
        "start:rollup": "rollup -c rollup.config.js -w",
        "start": "yarn start:rollup",
        "build:esbuild": "tsc --emitDeclarationOnly; NODE_ENV=production node build.js",
        "build": "rollup -c rollup.config.js"
    },
    "main": "es/index.js",
    "module": "es/index.js",
    "typings": "es/index.d.js",
    "files": [
        "es/*",
        "src/*",
        "dist/*"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@types/lodash": "^4.14.170"
    },
    "devDependencies": {},
    "babel": {
        "plugins": [
            "lodash"
        ],
        "presets": [
            "@babel/preset-react",
            "@babel/env"
        ]
    }
}